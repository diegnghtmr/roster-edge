# Frontend Environment Variables - Local Development
# This file is for local development. DO NOT commit to version control.

# API Configuration
VITE_API_BASE_URL=http://localhost:8080/api

# Application Configuration
VITE_APP_NAME=RosterEdge
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Sports Team Management Application

# Environment
VITE_ENV=development

# API Timeout (in milliseconds)
VITE_API_TIMEOUT=10000

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_LOGS=true
VITE_ENABLE_ERROR_REPORTING=false

# Authentication
VITE_SESSION_TIMEOUT=3600000
VITE_REFRESH_TOKEN_INTERVAL=1800000
